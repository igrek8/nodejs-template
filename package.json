  "keywords": [],
  "description": "",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "homepage": "https://github.com/igrek8/nodejs-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/igrek8/nodejs-template"
  },
  "bugs": {
    "url": "https://github.com/igrek8/nodejs-template/issues"
  },
  "files": [
    "./dist"
  ],
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "jest",
    "compile": "tsc --outDir ./dist --project ./tsconfig.build.json",
    "build": "npm run clean && npm run compile",
    "lint": "eslint \"**/*\"",
    "format": "prettier --check \"**/*\"",
    "docs:build": "typedoc --out ./docs",
    "docs:publish": "cp ./public/index.html ./docs/index.html && gh-pages --dist ./docs",
    "docs": "npm run docs:build && npm run docs:publish"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/github": "8.0.7",
    "@semantic-release/npm": "9.0.2",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/jest": "29",
    "@types/node": "18.14.1",
    "@typescript-eslint/eslint-plugin": "5.53.0",
    "@typescript-eslint/parser": "5.53.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-prettier": "4.2.1",
    "gh-pages": "5.0.0",
    "husky": "8.0.3",
    "jest": "29",
    "prettier": "2.8.4",
    "rimraf": "4.1.2",
    "semantic-release": "20.1.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typedoc": "0.23.25",
    "typedoc-plugin-markdown": "3.14.0",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "tslib": "^2"
  }
}
