{
  "name": "@igrek8/nodejs-template",
  "version": "0.0.0",
  "license": "MIT",
  "commitlint": {
    "extends": "@igrek8/semantic-release-config/commitlint.config.js"
  },
  "release": {
    "extends": "@igrek8/semantic-release-config"
  },
  "prettier": "@igrek8/prettier-config",
  "jest": {
    "preset": "@igrek8/jest-config"
  },
  "eslintConfig": {
    "extends": "@igrek8/eslint-config"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "./dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "clean": "rimraf ./dist",
    "test": "jest",
    "compile": "tsc --outDir ./dist --project ./tsconfig.build.json",
    "build": "npm run clean && npm run compile",
    "lint": "eslint \"**/*\"",
    "format": "prettier --check \"**/*\"",
    "docs": "typedoc-config build -o ./docs && typedoc-config publish -o ./docs"
  },
  "dependencies": {
    "tslib": "^2"
  },
  "devDependencies": {
    "@igrek8/eslint-config": "1.0.4",
    "@igrek8/jest-config": "1.0.2",
    "@igrek8/prettier-config": "1.0.1",
    "@igrek8/semantic-release-config": "1.1.0",
    "@igrek8/tsconfig": "1.0.1",
    "@igrek8/typedoc-config": "1.0.0",
    "husky": "8.0.3",
    "rimraf": "4.1.2"
  }
}
